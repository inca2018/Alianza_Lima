<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jesusinca.alianza">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".Activities.Inicio.MainActivity" />
        <activity
            android:name=".Activities.Captacion.CaptacionActivity"
            android:configChanges="screenSize|orientation" />
        <activity android:name=".Activities.Captacion.PruebaAnimacionActivity" />
        <activity android:name=".Activities.Captacion.RegistroPostulantesActivity" />
        <activity android:name=".Activities.Inicio.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.Inicio.PrincipalActivity"
            android:configChanges="screenSize|orientation" />
        <activity android:name=".Activities.Ubigeo.UbigeoActivity" />
        <activity android:name=".Activities.Captacion.ValidarDiagnosticoIndividualActivity" />
        <activity android:name=".Activities.Captacion.ListaMasivosActivity" />
        <activity android:name=".Activities.Captacion.MasivoNuevoActivity" />
        <activity android:name=".Activities.Captacion.ListaPersonaMasivoActivity" />
        <activity android:name=".Activities.Captacion.PersonaNuevoMasivoActivity"></activity>
    </application>

</manifest>